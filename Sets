a=set('abcdefgh')
b=set('abcdefghl')
print(b-a)

a=set('abcdefgh')
b=set('abcdefghl')
print(a|b)

a=set('abcdefgh')
b=set('abcdefghl')
print(a&b)

a=set('abcdefgh')
