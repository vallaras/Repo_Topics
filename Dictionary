d={}
type(d)
d={'a':4,'b':3,'c':7,'d':9}
print(d)
print(d['a'])
print(d['c'])
d.get('d')
d['b']=10

for i in d:
    print(d[i])

for x in d:
    print(x)

for x in d.values():
    print(x)

for x,y in d.items():
    print(x,y)

d={'a':4,'b':3,'c':7,'d':9}
del d['d']

d.pop('b')


